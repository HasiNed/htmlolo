{
  "name": "@hasined/onepagepdf",
  "version": "0.1.1",
  "description": "Convert Markdown or HTML to one-page-only PDF.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test:markdown && npm run test:html",
    "test:markdown": "onepagepdf -d README.md ./test/readme.pdf --temp ./test/readme.html --css ./test/github.css",
    "test:html": "onepagepdf -d https://github.com/HasiNed/onepagepdf ./test/github.pdf",
    "clean": "rm ./test/readme.pdf ./test/readme.html ./test/github.pdf"
  },
  "bin": {
    "onepagepdf": "onepagepdf.js"
  },
  "keywords": [
    "markdown-to-html",
    "markdown-to-pdf",
    "html-to-pdf",
    "markdown",
    "html",
    "pdf",
    "cli"
  ],
  "author": "Hasi Ned",
  "license": "MIT",
  "repository": "HasiNed/onepagepdf",
  "dependencies": {
    "@traptitech/markdown-it-katex": "^3.6.0",
    "@wooorm/starry-night": "^1.3.1",
    "commander": "^9.4.1",
    "file-url": "^4.0.0",
    "hast-util-to-html": "^8.0.3",
    "is-url": "^1.2.4",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-implicit-figures": "^0.10.0",
    "playwright": "^1.27.1",
    "url-parse": "^1.5.10"
  }
}
